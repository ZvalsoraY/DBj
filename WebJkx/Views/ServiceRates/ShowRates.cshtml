@model List<Entities.Rate>
@{
    ViewData["Title"] = "ShowRates";
}

<h1>Тарифы услуги</h1>

<table class="table">
    <tr>
        <th class="t">Id</th>
        <th class="t">Название</th>
        <th class="t">Цена за единицу</th>
        <th class="t">Дата начала работы</th>
        <th class="t">Дата окончания работы</th>
        <th class="t">Id услуги, к которой привязан тариф</th>
    </tr>

    @foreach (var rate in Model)
    {
        <tr>
            <td class="t">@rate.Id</td>
            <td class="t">@rate.Name</td>
            <td class="t">@rate.Price</td>
            <td class="t">@rate.StartData</td>
            <td class="t">@rate.EndData</td>
            <td class="t">@rate.ServiceId</td>
            
        </tr>
        
    }
</table>

<a asp-controller="ServiceRates" asp-action="Index" Class="button-my">Назад</a>
